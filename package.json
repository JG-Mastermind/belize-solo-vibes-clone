    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "build:monitor": "node scripts/build-monitor.mjs",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/",
    "build:report": "npm run build:monitor && echo 'ðŸ“Š Build report generated in build-reports/'",
