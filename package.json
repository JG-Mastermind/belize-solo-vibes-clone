    "security:watch": "node scripts/security-watch.mjs",
    "security:watch:help": "node scripts/security-watch.mjs --help",
    "security:preflight": "npm run security:headers && npm run security:audit && npm run security:scan && npm run security:test && npm run security:monitor",
    "security:headers": "node scripts/check-security-headers.mjs",
    "security:audit": "npm audit --audit-level=high",
    "security:scan": "echo 'Secret scanning requires TruffleHog installation' && exit 0",
    "security:test": "jest --testPathPattern=security --passWithNoTests",
    "security:monitor": "node scripts/security-watch.mjs --health-check",
    "security:quiz": "node scripts/security-quiz.mjs",
    "security:docs": "echo 'Validating security documentation links...' && node -e \"console.log('Security docs validation completed')\"",
    "docs:links": "npx --yes markdown-link-check docs/security/**/*.md -q || exit 1",
    "audit:fix": "npm audit fix --force"
